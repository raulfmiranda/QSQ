<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.blogspot.raulfmiranda.qsq">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_face"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_face_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.blogspot.raulfmiranda.qsq.FormActivity" android:screenOrientation="portrait"/>
        <activity android:name="com.blogspot.raulfmiranda.qsq.PesquisaActivity" android:screenOrientation="portrait"/>
        <activity android:name="com.blogspot.raulfmiranda.qsq.StartActivity" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>

